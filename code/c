# Variables
CC = clang
CFLAGS = -g -fsanitize=address

LIBFT_DIR = ../libft
LIBFT = $(LIBFT_DIR)/libft.a

INCLUDES = -I$(LIBFT_DIR)

SRC_DIR = ./src
SRCS =  $(SRC_DIR)/parseo.c\
                $(SRC_DIR)/aux.c\
                $(SRC_DIR)/delete_quotes.c\
                $(SRC_DIR)/expand_quotes.c\
