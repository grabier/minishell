        @$(CC) $(CFLAGS) $(OBJS) $(LIBFT) -lreadline -o $(NAME)
        @printf "$(GREEN)[Success] $(NAME) created successfully!\n$(RESET)"

$(OBJ_DIR)/%.o: $(SRC_DIR)/%.c | $(OBJ_DIR)
        @mkdir -p $(dir $@)
        @printf "$(YELLOW)[Compiling]$(RESET) $<\n"
        @$(CC) $(CFLAGS) $(INCLUDES) -c $< -o $@

